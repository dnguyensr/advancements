<div class="container">
  <div class="row pad-b-10">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
      <table class="table table-striped table-condensed">
        <thead>
          <tr>
            <th>Name</th>
            <th>Role</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.first_name %> <%= user.last_name %></td>
            <td><%= user.id %></td>
            <td>Edit</td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
